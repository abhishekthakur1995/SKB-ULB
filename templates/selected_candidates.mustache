<table class='table table-striped table-bordered table-hover table-condensed'>
	<thead>
		<tr>
			{{#lang}}
				<th class="text-align-center">#</th>
	            <th class="text-align-center">{{name}}</th>
	            <th class="text-align-center">{{guardian}}</th>
	            <th class="text-align-center">{{permanentAddress}}</th>
	            <th class="text-align-center">{{dob}}</th>
	            <th class="text-align-center">{{gender}}</th>
	            <th class="text-align-center">{{maritial_status}}</th>
	            <th class="text-align-center">{{category}}</th>
	            <th class="text-align-center">{{ulb_region}}</th>
	            <th class="text-align-center">{{special_preference_list}}</th>
	            <th class="text-align-center">{{receipt_number}}</th>
	        {{/lang}}    
		</tr>
	</thead>
	<tbody>
		{{#data}}
			<tr>
				<td class="text-align-center">{{serialNumber}}</td>
				<td class="text-align-center">{{name}}</td>
				<th class="text-align-center">{{guardian}}</th>
	            <th class="text-align-center">{{permanentAddress}}</th>
	            <th class="text-align-center">{{dob}}</th>
				<td class="text-align-center">{{#getTextInHindi}} {{gender}} {{/getTextInHindi}}</td>
				<td class="text-align-center">{{#getTextInHindi}} {{maritialStatus}} {{/getTextInHindi}}</td>
				<td class="text-align-center">{{#getTextInHindi}} {{category}} {{/getTextInHindi}}</td>
				<td class="text-align-center">{{ulbRegion}}</td>
				<td class="text-align-center">{{#getTextInHindi}} {{specialPreference}} {{/getTextInHindi}}</td>
				<td class="text-align-center">{{#getReceiptNumber}} {{receiptNumber}} {{/getReceiptNumber}}</td>
			</tr>
		{{/data}}

		{{^data}}
			<tr>
	    		<td colspan="11" class="text-align-center">No Record Found</td>
			</tr>
		{{/data}}
	</tbody>
</table>